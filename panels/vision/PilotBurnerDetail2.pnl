void EP_textFieldInCB(string dp1, bool bIgnStart,
                      string dp2, bool bIgnOn,
                      string dp3, unsigned uTry,
                      string dp4, unsigned uCD)
{

 float CD_Sec;
 string CD_str;
 CD_Sec = uCD / 1000.0;
 sprintf(CD_str,\"%3.1f\",CD_Sec);

  if (!bIgnStart) setValue(\"\", \"text\", \"\");
  else if (uTry == 0)  setValue(\"\", \"text\", \"ЗАПОЛНЕНИЕ МАГИСТРАЛИ ТГ ... \" + CD_str + \" сек\");
       else if (!bIgnOn) setValue(\"\", \"text\", \"ПОПЫТКА \" + uTry + \" : \" +  \"ОЖИДАНИЕ ПЛАМЕНИ ... \" + CD_str + \" сек\");
            else setValue(\"\", \"text\", \"ПОПЫТКА \" + uTry + \" : \" +  \"ЗАЖИГАНИЕ ... \" + CD_str + \" сек\");

}

