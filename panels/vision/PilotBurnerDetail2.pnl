    return;
  }
  if( !dpExists(\"gd\"+ $num + \".try_count:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }
  if( !dpExists(\"gd\"+ $num + \".countdown:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  dpConnect(\"EP_textFieldInCB\",
            \"gd\"+ $num + \".IgnitionStart:_online.._value\",
