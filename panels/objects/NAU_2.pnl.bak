"main()
{
  EP_setForeColor();
}

void EP_setForeColor()
{
  dyn_errClass err;

 if( !dpExists( $dpe_NAU+\".Level_Alarm:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  if( !dpExists( $dpe_NAU+\".SL1:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }


  dpConnect(\"EP_setForeColorCB\",
            $dpe_NAU+\".Level_Alarm:_online.._value\", $dpe_NAU+\".SL1:_online.._value\");
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");

}


void EP_setForeColorCB(string dpSource, bool boNewValueAlm, string dpSource2, float fLevel)
{
  if (boNewValueAlm)
  {
    setValue(\"\", \"foreCol\", \"White\");
    setValue(\"\", \"text\", \"АВАРИЙНЫЙ\");
    setValue(\"\", \"backCol\", \"Red\");
  }
  else
  {
    if (fLevel < 10.0)
    {
      setValue(\"\", \"foreCol\", \"Black\");
      setValue(\"\", \"text\", \"НИЗКИЙ\");
      setValue(\"\", \"backCol\", \"Yellow\");
    }
    else {
          setValue(\"\", \"foreCol\", \"Black\");
          setValue(\"\", \"text\", \"НОРМА\");
          setValue(\"\", \"backCol\", \"Green\");
         }
  }
}
