void EP_setBackColor()
{
  dyn_errClass err;

  if( !dpExists( $dpe_NAU + \".Mode:_online.._value\"))
  {
    setValue(\"\", \"color\", \"_dpdoesnotexist\");
    return;
  }

  dpConnect(\"EP_setBackColorCB\",
            $dpe_NAU + \".Mode:_online.._value\");
  err = getLastError();
  if (dynlen(err) > 0)
    setValue(\"\", \"color\", \"_dpdoesnotexist\");

}


void EP_setBackColorCB(string dpSource, bool boNewValue)
{
  if (!boNewValue)
    setValue(\"\", \"text\", \" АВТОМАТ \");
  else
    setValue(\"\", \"text\", \" НАЛАДКА \");
}

